{
  "name": "cubingitaly",
  "version": "1.0.0",
  "description": "Cubing Italy website",
  "main": "server/app.js",
  "license": "MIT",
  "scripts": {
    "start": "concurrently --kill-others \"npm run server:run\" \"ng serve --aot --progress=false --proxy-config proxy.conf.json\"",
    "lint:client": "ng lint",
    "lint:server": "tslint './server/**/*.ts' -c ./server/tslint.json --fix",
    "test:client": "ng test --watch false",
    "test:server": "mocha -r ts-node/register ./server/**/*.spec.ts",
    "test:swc": "nyc -r lcov -e .ts -x \"*.spec.ts\" mocha -r ts-node/register ./server/**/*.spec.ts && nyc report",
    "e2e:client": "ng e2e",
    "build": "ng build --prod --aot --output-path=dist/client && npm run server:build",
    "server:run": "nodemon $NODE_DEBUG_OPTION",
    "server:build": "tsc -p ./server",
    "postinstall": "npm run build"
  },
  "authors": "Cubing Italy Team Informatico",
  "engines": {
    "node": "8.9.1",
    "npm": "5.2.0"
  },
  "repository": {
    "url": "https://github.com/CubingItaly/cubingitaly.org.git"
